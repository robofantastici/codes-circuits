//second codes of fifth circuit
#include <LiquidCrystal.h>
const int pot=A0;
float value;

const int rs = 12, en = 11, d4 = 5, d5 = 4, d6 = 3, d7 = 2;
LiquidCrystal lcd(rs, en, d4, d5, d6, d7);

void setup() {

  lcd.begin(16, 2);
  pinMode(pot,INPUT);
}

void loop() {

    lcd.setCursor(0, 0);

    value=analogRead(pot);
    lcd.print("Value is: ");
    lcd.print(value);

    delay(500);



}
